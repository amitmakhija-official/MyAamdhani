                                                <div class="card-body">
                                                    <p>no reviews yet</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4">
                        <div class="product-right product-form-box">
                            <h4><del>&#8377; @Model.MRPPrice.ToString().CommaSeperated()</del><span>@CommonFunction.GetAmountDifference(Model.MRPPrice.GetValueOrDefault(), Model.PiecePrice.GetValueOrDefault()).Replace("-", "").Substring(0, 4)% off</span></h4>
                            <h3>&#8377; @Model.PiecePrice.ToString().CommaSeperated()</h3>
                            <ul class="color-variant">
                                @foreach (var item in Model.ImageList)
                                {
                                    <li style="background-color: #@item.Color"></li>
                                }
                            </ul>
                            <div class="product-description border-product">

                                <h6 class="product-title size-text">select size</h6>
                                <div class="modal fade" id="sizemodal" tabindex="-1" role="dialog"
                                     aria-labelledby="exampleModalLabel" aria-hidden="true">
                                    <div class="modal-dialog modal-dialog-centered" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h5 class="modal-title" id="exampleModalLabel">Sheer Straight Kurta</h5>
                                                <button type="button" class="close" data-dismiss="modal"
                                                        aria-label="Close">
                                                    <span aria-hidden="true">&times;</span>
                                                </button>
                                            </div>
                                            <div class="modal-body">
                                                <img src="~/Content/Images/size-chart.jpg" alt=""
                                                     class="img-fluid blur-up lazyload">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="qty-box">
                                    <select class="form-control" id="drp_size" name="size">
                                        @foreach (var item in CommonFunction.GetSize())
                                        {
                                            <option value="@item.Value" selected="@item.Selected">@item.Text</option>
                                        }
                                    </select>
                                </div>
                                <h6 class="product-title">quantity</h6>
                                <div class="qty-box">
                                    <select class="form-control" id="drp_qty" name="quantity">
                                        @foreach (var item in CommonFunction.CountListDropdown())
                                        {
                                            <option value="@item.Value" selected="@item.Selected">@item.Text</option>
                                        }
                                    </select>

                                </div>
                            </div>
                            <div class="product-buttons">

                                <button type="button" data-productkey="@Model.ProductUniqueKey" id="btn_addtocart" class="btn btn-solid">add to cart</button> <button type="button" class="btn btn-solid">buy now</button>
