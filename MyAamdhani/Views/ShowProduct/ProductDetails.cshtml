            <div class="container">
                <div class="row">
                    <div class="col-lg-4">
                        <div class="product-slick">
                            <div>
                                <img src="@Model.CoverImage" alt=""
                                     class="img-fluid blur-up lazyload image_zoom_cls-0">
                            </div>
                            <div>
                                <img src="@Model.CoverImage" alt=""
                                     class="img-fluid blur-up lazyload image_zoom_cls-1">
                            </div>
                            <div>
