                            <div class="col-12 p-0">
                                <div class="slider-nav">
                                    @if (Model.ImageList != null)
                                    {
                                        foreach (var image in Model.ImageList)
                                        {
                                            <div>
                                                <img src="@image.ImagePath" alt=""
                                                     class="img-fluid blur-up lazyload">
                                            </div>
                                        }
                                    }


                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4">
